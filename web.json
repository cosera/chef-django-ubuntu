{
    "run_list": [ "recipe[main::default]", "recipe[nginx::default]" ],
    "user": {
        "default": {
            "username": "manuel",
            "home_directory": "/home/manuel/"
        }
    },
   "python": {
      "virtualenv_name": "django"
  },
  "nginx": {
      "version": "1.2.3",
      "configure_arguments": "",
      "dir": "/etc/nginx",
      "log_dir": "/var/log/nginx",
      "binary": "/usr/sbin/nginx",
      "user": "www-data",
      "pid": "/var/run/nginx.pid",
      "gzip": "on",
      "gzip_http_version": "1.0",
      "gzip_comp_level": "2",
      "gzip_proxied": "any",
      "daemon_disable": "true",
      "gzip_types": [
            "text/plain",
            "text/html",
            "text/css",
            "application/x-javascript",
            "text/xml",
            "application/xml",
            "application/xml+rss",
            "text/javascript",
            "application/javascript",
            "application/json"
      ],
      "server": {
         "listen": "8080",
         "server_name": "localhost"
       },
      "keepalive": "on",
      "keepalive_timeout": 65,
      "worker_processes": 2,
      "worker_connections": 1024,
      "server_names_hash_bucket_size": 64,
      "disable_access_log": "false",
      "default_site_enabled": "true",
      "source": {
          "url": "http://www.nginx.org/download/nginx-1.2.3.tar.gz",
          "modules": [
            "http_ssl_module",
            "http_gzip_static_module"
          ]
       }
   }
}
